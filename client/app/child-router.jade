template
  section.au-animate
    h2 ${heading}
    div
      .col-md-2
        ul.well.nav.nav-pills.nav-stacked
          li(repeat.for='row of router.navigation', class="${row.isActive ? 'active' : ''}")
            a(href.bind='row.href') ${row.title}
      .col-md-10(style='padding: 0')
        router-view
