    "compilerOptions": {
        "module": "system",
        "target": "ES5",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "experimentalDecorators": true,
        "noEmitOnError": false
    },
    "exclude": [
        "node_modules",
        ".meteor"
    ]
}